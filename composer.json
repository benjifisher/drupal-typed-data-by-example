{
    "name": "mglaman/drupal-typed-data-by-example",
    "description": "Examples using the Typed Data API in Drupal",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Matt Glaman",
            "email": "nmd.matt@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "drupal/core": "^9.3"
    },
    "autoload": {
        "files": [
            "bootstrap.php",
            "helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "drupal/coder": "^8.3"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
